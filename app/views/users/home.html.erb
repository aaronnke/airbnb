<div id="hero-image"> </div>

<div id="search-bar-underlay"> </div>



<%= form_for :search, url: search_path, method: :get  do |form| %>

	<%= form.text_field :query, id: "search-bar-input", required: true %>
	<%= form.text_field :daterange, id: "search-dates" %>
	<%= form.submit 'Search', id: "search-bar-submit" %>

<% end %>